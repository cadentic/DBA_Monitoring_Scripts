# Oracle Executable Tests
#------------------------------------------------------------------------------

=head2 O_US - Oracle Executable Owned by Different Users of a Same Group

Lists Oracle executable occurrences that belong to a same group but having
different owners. This check is applicable for UNIX only.

=cut

macro OR_user
{import $TOP

 debug ' Inside TLsecure module, check for same group with different users'
 write '---+ Oracle Executable Owned by Different Users of a Same Group'
 write
 prefix
  write '| *Group ID*| *User ID*|*Executable*|'
 var %tbl = ()
 loop $fil ('/etc/oratab','/var/opt/oracle/oratab')
 {loop $lin (grepFile($fil,'^[^#]'))
  {var (undef,$hom) = split(':',$lin,3)
   if @sta = getStat(catFile($hom,'bin','oracle'))
    var $tbl{$sta[5],$sta[4],encode(lastFile())} = 1
  }
 }
 loop $gid (keys(%tbl,'IA'))
 {var $cnt = @uid = keys($tbl{$gid},'IA')
  if expr('>',$cnt,1)
  {loop $uid (@uid)
    write '| ',$gid,'| ',$uid,'|',join('%BR%',keys($tbl{$gid,$uid})),' |'
  }
 }
 if !hasOutput(true)
  write 'No occurrences found%BR%'
 write $TOP
}

#------------------------------------------------------------------------------
# Main program
#------------------------------------------------------------------------------

echo tput('bold'),'Identification of Potential Security Risks ...',tput('off')

call setAbbr('TOOL_SEC_')

report risks
prefix
{write '---+!! Potential Security Risks'
 write $TOC
}

# Get the check list
if !@arg
{call requestInput('TLsecure')
 var @opt = @{RUN.REQUEST.W_LIST}
}
elsif match($arg[0],'^(\w+(,\w+))$')
 var @opt = split(',',first)
else
 var @opt = @arg

# Check database connection
if grep(@opt,'^(ALL|D_\w+)$','f')
{if ${SET.DB.DB.I_DB}
  call setSqlTarget(last)
 if testSql()
 {echo 'Cannot connect to the database: ',getSqlMessage()
  write 'Cannot connect to the database (',getSqlMessage(),')'
 }
 else
  var $DBVER = get_db_version(false,'10')
}

# Perform the tests
var ($lsn,$net) = ()
loop $opt (@opt)
{if member($opt,'D_DP','ALL')
  call DB_default_pwd()
 if member($opt,'D_KP','ALL')
  call DB_known_pwd()
 if member($opt,'D_NV','ALL')
  call DB_not_visible()
 if member($opt,'D_OS','ALL')
  call DB_os_password()
 if member($opt,'D_PG','ALL')
  call DB_grants()
 if member($opt,'D_PW','ALL')
  call DB_pwfile()
 if member($opt,'D_ST','ALL')
  call DB_system()

 if member($opt,'L_NP','ALL')
  var $lsn = join('|','np',$lsn)
 if member($opt,'L_PL','ALL')
  var $lsn = join('|','pl',$lsn)
 if member($opt,'L_RT','ALL')
  var $lsn = join('|','rt',$lsn)

 if member($opt,'N_AS','ALL')
  var $net = join('|','as',$net)

 if isUnix()
 {if member($opt,'O_US','ALL')
   call OR_user()
 }
}
if $lsn
 call LSN_checks($lsn)
if $net
 call NET_checks($net)

# Render the report
if isCreated(true)
{call getGroupFile('D_CWD',renderFile())
 echo 'Result file: ',last
}
else
 echo 'No results'

